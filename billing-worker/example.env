# Vercel
# Create a token at: https://vercel.com/account/tokens
VERCEL_TOKEN=

# Cloudflare
# Create a token at: https://dash.cloudflare.com/profile/api-tokens
# Template: Use "Read Analytics" template or Custom Token with Account > Account Analytics > Read
CLOUDFLARE_API_TOKEN=
CLOUDFLARE_ACCOUNT_ID=

# Google Cloud
# JSON content of your service account key file.
# Ensure the service account has "Monitoring Viewer" role.
GOOGLE_SERVICE_ACCOUNT_JSON=

# Resend
# Get your API key from: https://resend.com/api-keys
RESEND_API_KEY=

# Configuration
ADMIN_EMAIL=your-email@example.com

